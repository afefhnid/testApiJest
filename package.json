{
  "name": "api-kline",
  "version": "0.0.0",
  "description": "Kline",
  "main": "src/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon src/index.js",
    "start": "node src/index.js",
    "pretest": "cross-env NODE_ENV=test run-s migrate seed",
    "migrate": "sequelize db:migrate",
    "seed": "sequelize db:seed:all",
    "lint": "eslint . && prettier --check \"**/*.{json,md,html,yaml,css}\"",
    "lint:fix": "eslint . --fix && prettier --write \"**/*.{json,md,html,yaml,css}\"",
    "test": "jest ",
    "test:watch": "jest --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,md,html,yaml,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@kazaar/express-error-handler": "^2.5.0",
    "body-parser": "^1.19.0",
    "celebrate": "^12.0.0",
    "chai-http": "^4.3.0",
    "convict": "^5.2.0",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "express-promise-router": "^3.0.3",
    "fs": "0.0.1-security",
    "helmet": "^3.15.0",
    "http-errors": "^1.7.2",
    "lowdb": "^1.0.0",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "mysql2": "^2.1.0",
    "path": "^0.12.7",
    "path.join": "^1.0.0",
    "sequelize": "^5.21.5",
    "sequelize-cli": "^5.5.1",
    "storage": "^0.2.0",
    "uuid": "^7.0.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-chai-expect": "^2.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.0.1",
    "husky": "^1.3.1",
    "jest": "^25.3.0",
    "lint-staged": "^8.1.3",
    "mocha": "^7.1.0",
    "nodemon": "^1.18.9",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.0.0",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2"
  },
  "engines": {
    "node": ">=10.16"
  },
  "jest": {
    "verbose": true,
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ]
  }
}
